<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.blink.SignUpController">
   <children>
      <VBox fx:id="paneSignUp" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label text="Sign In" />
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="First name:" />
                  <Label text="Last Name:" GridPane.rowIndex="1" />
                  <Label text="Email:" GridPane.rowIndex="2" />
                  <Label text="Username:" GridPane.rowIndex="3" />
                  <Label text="Password:" GridPane.rowIndex="4" />
                  <TextField fx:id="fname" GridPane.columnIndex="1" />
                  <TextField fx:id="lname" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="emailadd" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="uname" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="pass" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <Button fx:id="register" mnemonicParsing="false" onAction="#register" text="Register" />
         </children>
      </VBox>
   </children>
</AnchorPane>
